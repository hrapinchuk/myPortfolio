    <div id="map"></div>
    <script type="text/javascript">

function initMap() {

  var map = new google.maps.Map(document.getElementById('map'), {
    zoom: 2,
    center: {lat: 0, lng: 0}
  });
  
  var locations = [
    ['Seoul', 37.566535, 126.9779692],
    ['Galway', 53.270668, -9.0567905],
    ['Machu Picchu', -13.1631412, -72.5449629]
  ];
  
  var marker, index;
  
  for (index = 0; index < locations.length; index++) {  
    marker = new google.maps.Marker({
      position: new google.maps.LatLng(locations[index][1], locations[index][2]),
      map: map
    });
  }
}

    </script>
    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAaV2-25JbspbWajKes4Vzyi0mdpmNfeWA&callback=initMap">
    </script>
